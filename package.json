  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@types/bcryptjs": "^2.4.4",
    "@types/node": "^20.7.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
